class Compute 
{
    static pair getMinMax(long a[], long n)  
    {
        //Write your code here
        long Min=a[0];
        long Max=a[0];
        for(int i=0;i<n;i++){
            if(a[i]>Max){
                Max=a[i];
            }
            if(a[i]<Min){
                Min=a[i];
            }
        }
        return new pair(Min,Max);
        
        
    }
}
